        <div class="progress mt-3">
            <div class="progress-bar" role="progressbar" style="width: 50%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><span class="p-2 bg-light alert-secondary">Dados Básicos</span></div>
            <div class="progress-bar" role="progressbar" style="width: 50%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><span class="p-2 bg-light alert-secondary">Endereço</span></div>

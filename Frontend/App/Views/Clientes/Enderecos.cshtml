   <div class="container">
        <div class="mt-2">
        <div class="float-left">
            <a asp-action="DadosBasicos" class="btn btn-link"><em class="fas fa-arrow-left"></em> Voltar</a>
        </div>
        <div class="float-right">
            <a asp-action="Contatos" class="btn btn-link">Próximo <em class="fas fa-arrow-right"></em></a>
        </div>
        </div>
        <br>
        <div class="progress mt-3">
            <div class="progress-bar" role="progressbar" style="width: 50%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><span class="p-2 bg-light alert-secondary">Dados Básicos</span></div>
            <div class="progress-bar" role="progressbar" style="width: 50%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">Endereço</div>
            <div class="progress-bar" role="progressbar" style="width: 50%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><span class="p-2 bg-light alert-secondary">Contato</span></div>
        </div>
        <div class="card card-body mt-2">
            <form id="form" method="POST">
                <div class="form-group">
                    <label for="nome">Cep</label>
                    <input class="form-control" id="cep" name="cep" type="text">
                </div>
                <div class="form-group">
                    <label for="nome">Rua</label>
                    <input class="form-control" id="rua" name="rua" type="text">
                </div>
                <div class="form-group">
                    <label for="nome">Bairro</label>
                    <input class="form-control" id="bairro" name="bairro" type="text">
                </div>
                <div class="form-group">
                    <label for="nome">Numero</label>
                    <input class="form-control" id="numero" name="numero" type="text">
                </div>
                <div class="float-right">
                    <input class="btn btn-success" type="submit" value="Salvar">
                </div>
            </form>
        </div>
    </div>